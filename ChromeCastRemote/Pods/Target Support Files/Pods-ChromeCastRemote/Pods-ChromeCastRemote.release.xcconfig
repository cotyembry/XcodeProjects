FRAMEWORK_SEARCH_PATHS = $(inherited) "${PODS_ROOT}/google-cast-sdk/GoogleCastSDK-2.10.0-Release"
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = $(inherited) "${PODS_ROOT}/Headers/Public" "${PODS_ROOT}/Headers/Public/google-cast-sdk"
LD_RUNPATH_SEARCH_PATHS = $(inherited) '@executable_path/Frameworks' '@loader_path/Frameworks'
OTHER_CFLAGS = $(inherited)  -isystem "${PODS_ROOT}/Headers/Public" -isystem "${PODS_ROOT}/Headers/Public/google-cast-sdk"
OTHER_LDFLAGS = $(inherited) -ObjC -l"c++" -framework "AVFoundation" -framework "Accelerate" -framework "AudioToolbox" -framework "CFNetwork" -framework "CoreBluetooth" -framework "CoreText" -framework "GoogleCast" -framework "MediaPlayer" -framework "Security" -framework "SystemConfiguration"
PODS_FRAMEWORK_BUILD_PATH = $(BUILD_DIR)/$(CONFIGURATION)$(EFFECTIVE_PLATFORM_NAME)/Pods-ChromeCastRemote
PODS_ROOT = ${SRCROOT}/Pods